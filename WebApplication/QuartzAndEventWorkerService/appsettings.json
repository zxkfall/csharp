{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "CORN-Scheduler": {
    "CronExpression": "0/2 * * * * ?"
  },
  "Interval-Scheduler": {
    "IntervalInSeconds": 10
  },
  "Quartz": {
    "SchedulerName": "QuartzScheduler"
  },
  "EventStore": {
    "ConnectionString": "esdb://127.0.0.1:2113?tls=false&keepAliveTimeout=10000&keepAliveInterval=10000",
    "ConnectionName": "EventStoreConnection"
  }
}
