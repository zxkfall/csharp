{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DbConnectionString": "Data Source=localhost;Initial Catalog=test;Integrated Security=SSPI;Encrypt=True;Trust Server Certificate=True;",
  "EventStore": {
    "ConnectionString": "esdb://127.0.0.1:2113?tls=false&keepAliveTimeout=10000&keepAliveInterval=10000",
